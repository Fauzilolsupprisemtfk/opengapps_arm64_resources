<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <stroke android:width="@dimen/quartz_shopping_offer_image_button_line_thickness" android:color="@color/quartz_shopping_offer_image_button_color" />
        </shape>
    </item>
    <item>
        <shape android:shape="line">
            <stroke android:width="@dimen/quartz_shopping_offer_image_button_line_thickness" android:color="@color/quartz_shopping_offer_image_button_color" />
        </shape>
    </item>
    <item>
        <rotate android:fromDegrees="90.0" android:toDegrees="-90.0" android:pivotX="50.0%" android:pivotY="50.0%">
            <shape android:shape="line">
                <stroke android:width="@dimen/quartz_shopping_offer_image_button_line_thickness" android:color="@color/quartz_shopping_offer_image_button_color" />
            </shape>
        </rotate>
    </item>
</layer-list>