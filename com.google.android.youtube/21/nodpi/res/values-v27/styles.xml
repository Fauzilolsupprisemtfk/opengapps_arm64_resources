    <style name="Base.V27.Theme.YouTube.Dark" parent="@style/Base.Theme.YouTube.Dark">
        <item name="android:navigationBarColor">@color/navigationbar_background_dark</item>
        <item name="android:windowLightNavigationBar">false</item>
        <item name="android:navigationBarDividerColor">@color/navigationbar_divider_dark</item>
    </style>
    <style name="Base.V27.Theme.YouTube.GalleryActivity" parent="@style/Base.Theme.YouTube.GalleryActivity">
