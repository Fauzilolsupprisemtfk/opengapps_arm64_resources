<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Auth.Theme.Setup.D2D.Source" parent="@style/Theme.AppCompat.Light">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@android:color/transparent</item>
        <item name="android:windowAnimationStyle">@android:style/Animation</item>
        <item name="android:windowDisablePreview">true</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:statusBarColor">@android:color/transparent</item>
        <item name="android:navigationBarColor">@android:color/transparent</item>
    </style>
    <style name="Base.Theme.AppCompat" parent="@style/Base.V22.Theme.AppCompat" />
    <style name="Base.Theme.AppCompat.Light" parent="@style/Base.V22.Theme.AppCompat.Light" />
    <style name="SuwAlertDialogTheme" parent="@android:style/Theme.DeviceDefault.Dialog.Alert" />
    <style name="SuwAlertDialogTheme.Light" parent="@android:style/Theme.DeviceDefault.Light.Dialog.Alert" />
    <style name="SuwThemeGlifV2" parent="@style/SuwThemeGlif">
        <item name="android:colorBackground">@color/suw_glif_background_color_dark</item>
        <item name="android:editTextStyle">@style/SuwEditText</item>
        <item name="android:alertDialogTheme">@style/SuwAlertDialogTheme</item>
        <item name="android:windowLightStatusBar">false</item>
        <item name="suwDividerInsetEnd">?suwMarginSides</item>
        <item name="suwDividerInsetStart">?suwMarginSides</item>
        <item name="suwDividerInsetStartNoIcon">?suwMarginSides</item>
        <item name="suwEditTextBackgroundColor">@color/suw_glif_edit_text_bg_light_color</item>
        <item name="suwGlifHeaderGravity">center_horizontal</item>
        <item name="suwScrollIndicators">bottom|top</item>
        <item name="suwBackgroundPatterned">false</item>
        <item name="suwBackgroundBaseColor">?android:colorBackground</item>
    </style>
    <style name="SuwThemeGlifV2.Light" parent="@style/SuwThemeGlif.Light">
        <item name="android:colorBackground">@color/suw_glif_background_color_light</item>
        <item name="android:editTextStyle">@style/SuwEditText</item>
        <item name="android:alertDialogTheme">@style/SuwAlertDialogTheme.Light</item>
        <item name="android:windowLightStatusBar">true</item>
        <item name="suwDividerInsetEnd">?suwMarginSides</item>
        <item name="suwDividerInsetStart">?suwMarginSides</item>
        <item name="suwDividerInsetStartNoIcon">?suwMarginSides</item>
        <item name="suwEditTextBackgroundColor">@color/suw_glif_edit_text_bg_light_color</item>
        <item name="suwGlifHeaderGravity">center_horizontal</item>
        <item name="suwScrollIndicators">bottom|top</item>
        <item name="suwBackgroundPatterned">false</item>
        <item name="suwBackgroundBaseColor">?android:colorBackground</item>
    </style>
    <style name="Base.V22.Theme.AppCompat" parent="@style/Base.V21.Theme.AppCompat">
        <item name="actionModeShareDrawable">?android:actionModeShareDrawable</item>
        <item name="editTextBackground">?android:editTextBackground</item>
    </style>
    <style name="Base.V22.Theme.AppCompat.Light" parent="@style/Base.V21.Theme.AppCompat.Light">
        <item name="actionModeShareDrawable">?android:actionModeShareDrawable</item>
        <item name="editTextBackground">?android:editTextBackground</item>
    </style>
</resources>
